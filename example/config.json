{
  "Log": {
    "Level": "info",
    "Output": ""
  },
  "Cores": [
    {
      "Type": "sing",
      "Log": {
        "Level": "info",
        "Timestamp": true
      },
      "NTP": {
        "Enable": false,
        "Server": "time.apple.com",
        "ServerPort": 0
      },
      "OriginalPath": "/etc/N2X/sing_origin.json"
    }
  ],
  "Nodes": [
    {
      "Core": "sing",
      "ApiHost": "${N2X_API_HOST:-http://127.0.0.1}",
      "ApiKey": "${N2X_API_KEY}",
      "NodeID": 33,
      "NodeType": "shadowsocks",
      "Timeout": 30,
      "ListenIP": "0.0.0.0",
      "SendIP": "0.0.0.0",
      "DeviceOnlineMinTraffic": 200,
      "MinReportTraffic": 0,
      "TCPFastOpen": false,
      "SniffEnabled": true,
      "CertConfig": {
        "CertMode": "self",
        "RejectUnknownSni": false,
        "CertDomain": "${N2X_CERT_DOMAIN:-example.com}",
        "CertFile": "/etc/N2X/fullchain.cer",
        "KeyFile": "/etc/N2X/cert.key",
        "Provider": "${N2X_CERT_PROVIDER:-cloudflare}",
        "Email": "${N2X_CERT_EMAIL:-}",
        "DNSEnv": {
          "CF_API_KEY": "${CF_API_KEY:-}",
          "CLOUDFLARE_EMAIL": "${CLOUDFLARE_EMAIL:-}"
        }
      }
    }
  ]
}
